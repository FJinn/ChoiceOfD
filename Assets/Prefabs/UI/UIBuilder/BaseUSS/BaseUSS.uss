.baseButton {
    flex-basis: 100%;
    background-color: rgba(188, 188, 188, 0);
    font-size: 25px;
    -unity-font-style: bold;
}

.baseButtonPointerEnter {
    transition-property: background-color;
    transition-duration: 0.2s;
    transition-delay: 0.05s;
    background-color: rgba(255, 255, 255, 0.46);
}
